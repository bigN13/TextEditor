<orchestra:DocumentView x:Class="Orchestra.Modules.TextEditorModule.Views.PropertiesView"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:catel="http://catel.codeplex.com"
                        xmlns:orchestra="http://orchestra.codeplex.com"
                        >
    <!-- Content -->
    <catel:StackGrid>
        <StackPanel>
            <TextBlock Text="Url" />
            <TextBlock Text="{Binding Url}" />
            <TextBlock Text="Regex Pattern" />
            <TextBox />
            <TextBlock Text="Document Map" />
            <ListView ItemsSource="{Binding MethodSignatureCollection}">
                
            </ListView>

        </StackPanel>
    </catel:StackGrid>
</orchestra:DocumentView>
